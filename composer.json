{
    "name": "fdevs/photatoes",
    "type": "library",
    "description": "An object oriented hosting Image library",
    "keywords": [],
    "homepage": "http://4devs.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Samusev Andrey",
            "email": "andrey_simfi@list.ru"
        }
    ],
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "guzzle/guzzle": "~3.7",
        "symfony/serializer": "~2.4|~3.0"
    },
    "suggest": {
        "guzzle/guzzle": "for the Yandex Adapter",
        "symfony/serializer": "for the Yandex Adapter"
    },
    "autoload": {
        "psr-4": {
            "FDevs\\Photatoes\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "FDevs\\Photatoes\\Tests\\": "tests/"
        }
    }
}
